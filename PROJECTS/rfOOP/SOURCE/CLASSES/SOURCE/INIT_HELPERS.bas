!>-------------------------------------------------------------------------------
!!
{
"FUNCTION": "INIT_CLASS_NAME$(INIT_CLASS, PROPERTY$, TYPINGS, CLASS_NAME$)",
"DESCRIPTION":
"
HELPER FUNCTION FOR INIT_CLASS(MM_ADDRESS$)
"
}
!!


FN.DEF INIT_CLASS_NAME$(INIT_CLASS, PROPERTY$, TYPINGS, CLASS_NAME$)

   BUNDLE.PUT    INIT_CLASS, PROPERTY$, CLASS_NAME$
   BUNDLE.PUT    TYPINGS, PROPERTY$, "S"
   
   EVENT$ = "{EVENT:{FUNCTION: " + CHR$(34) + "INIT_CLASS_NAME$()" + CHR$(34) + ", TYPE: " + CHR$(34) + "CLASS PROPERTY 'NAME' INITIALIZATION SUCCESS" + CHR$(34) + ", MESSAGE: " + CHR$(34) + "SUCCESSFULLY INITIALIZED CLASS PROPERTY 'NAME' AS '" + CLASS_NAME$ + "'" + CHR$(34) + "}}" 
   LOG_EVENT$(EVENT$)
   
FN.END


