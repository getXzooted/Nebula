!>-------------------------------------------------------------------------------
!!
{
"FUNCTION": "INIT_CLASS_CLASSVARIABLES$(INIT_CLASS, PROPERTY$, TYPINGS, CLASS_NAME$)",
"DESCRIPTION":
"
HELPER FUNCTION FOR INIT_CLASS(MM_ADDRESS$)
"
}
!!


FN.DEF INIT_CLASS_CLASSVARIABLES$(INIT_CLASS, PROPERTY$, TYPINGS, CLASS_NAME$)

   CLASS_VARIABLES$ = MM_CREATE$("B")
   CLASS_VARIABLES  = MM_POINTER(CLASS_VARIABLES$)
   
   BUNDLE.PUT    CLASS_VARIABLES, "CLASS NAME", CLASS_NAME$
   BUNDLE.PUT    INIT_CLASS, PROPERTY$, CLASS_VARIABLES$
   BUNDLE.PUT    TYPINGS, PROPERTY$, "MA"

   EVENT$ = "{EVENT:{FUNCTION: " + CHR$(34) + "INIT_CLASS_SUBTYPE$()" + CHR$(34) + ", TYPE: " + CHR$(34) + "CLASS PROPERTY 'CLASS VARIABLES' INITIALIZATION SUCCESS" + CHR$(34) + ", MESSAGE: " + CHR$(34) + "SUCCESSFULLY INITIALIZED CLASS PROPERTY 'CLASS VARIABLES' AS '" + CLASS_VARIABLES$ + "' FOR CLASS NAMED '" + CLASS_NAME$ + "'" + CHR$(34) + "}}" 
   LOG_EVENT$(EVENT$)

FN.END


